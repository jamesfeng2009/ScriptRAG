{
  "dashboard": {
    "title": "RAG Screenplay Multi-Agent System",
    "tags": ["screenplay", "multi-agent", "rag"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Workflow Executions",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(workflow_executions_total[5m])",
            "legendFormat": "{{status}}"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 2,
        "title": "Workflow Duration (p95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(workflow_duration_seconds_bucket[5m]))",
            "legendFormat": "p95"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 3,
        "title": "Agent Executions by Status",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(agent_executions_total[5m])",
            "legendFormat": "{{agent_name}} - {{status}}"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 4,
        "title": "Agent Execution Duration (p95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(agent_execution_duration_seconds_bucket[5m]))",
            "legendFormat": "{{agent_name}}"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 5,
        "title": "LLM Calls by Provider",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(llm_calls_total[5m])",
            "legendFormat": "{{provider}} - {{model}} - {{status}}"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 16,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 6,
        "title": "LLM Call Duration (p95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(llm_call_duration_seconds_bucket[5m]))",
            "legendFormat": "{{provider}} - {{model}}"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 16,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 7,
        "title": "LLM Token Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(llm_tokens_total[5m])",
            "legendFormat": "{{provider}} - {{token_type}}"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 24,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 8,
        "title": "Retrieval Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(retrieval_operations_total[5m])",
            "legendFormat": "{{operation_type}} - {{status}}"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 24,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 9,
        "title": "Documents Retrieved",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(documents_retrieved_total[5m])",
            "legendFormat": "{{workspace_id}}"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 32,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 10,
        "title": "Workflow Pivots",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(workflow_pivots_total[5m])",
            "legendFormat": "{{pivot_reason}}"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 32,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 11,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(errors_total[5m])",
            "legendFormat": "{{component}} - {{error_type}}"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 40,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 12,
        "title": "Active Tasks",
        "type": "graph",
        "targets": [
          {
            "expr": "active_tasks",
            "legendFormat": "Active Tasks"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 40,
          "w": 12,
          "h": 8
        }
      }
    ],
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    }
  }
}
